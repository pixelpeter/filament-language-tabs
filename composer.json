{
    "name": "pixelpeter/filament-language-tabs",
    "description": "Group multilingual fields into tabs",
    "keywords": [
        "pixelpeter",
        "laravel",
        "filament-language-tabs"
    ],
    "homepage": "https://github.com/pixelpeter/filament-language-tabs",
    "license": "MIT",
    "authors": [
        {
            "name": "pixelpeter",
            "email": "info@pixelpeter.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "filament/filament": "^2.0",
        "laravel/framework": "9.*|10.*|11.*|12.*",
        "spatie/laravel-package-tools": "^1.13.5"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "7.*|8.*|9.*|10.*",
        "nunomaduro/collision": "^6.0|^7.0|^8.0",
        "nunomaduro/larastan": "^2.0.1|^3.0",
        "pestphp/pest": "^1.21|^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^1.1|^2.0|^3.0",
        "pestphp/pest-plugin-livewire": "^1.0|^2.0|^3.0",
        "pestphp/pest-plugin-parallel": "^0.3|^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0|^12.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Pixelpeter\\FilamentLanguageTabs\\": "src",
            "Pixelpeter\\FilamentLanguageTabs\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pixelpeter\\FilamentLanguageTabs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentLanguageTabsServiceProvider"
            ],
            "aliases": {
                "LanguageTabs": "Pixelpeter\\LanguageTabs\\Facades\\LanguageTabs"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
